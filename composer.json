{
    "name": "laravel-notification-channels/lob",
    "description": "Lob.com notifications channel for Laravel 5.3",
    "homepage": "https://github.com/laravel-notification-channels/lob",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "themsaid@gmail.com",
            "homepage": "http://themsaid.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3@dev",
        "illuminate/queue": "^5.3@dev",
        "illuminate/support": "^5.3@dev",
        "lob/lob-php": "^1.6"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Lob\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Lob\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
